<html>
<head>
<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="">
	<meta name="author" content="">

	<script src="js/jquery-1.12.0.js"></script>
	<!-- script src="js/jquery.mobile-1.4.5.js"></script -->

	<title>Camera Remote</title>


	<script>
		console.log("Loading...");

		$( document ).ready(function() {
			
		});

	</script>



</head>
<body>

    <div id="wrapper">

        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
		<li class="sidebar-brand">
			<a href="#" class="">
				Menu
			</a>
		</li>
		<li>
			<a href="index.html">Take Picture</a>
		</li>
		<li>
			<a href="photos.html">Photos</a>
		</li>
		<li>
			<a href="test.html">TEST</a>
		</li>
            </ul>
        </div>
<a href="photos.html">PHOTOS</a>
        <!-- /#sidebar-wrapper -->

	<!-- Page Content -->
	<div id="page-content-wrapper">
		<div class="row">
			<div class="col-xs-5">
				<a href="#menu-toggle" class="btn btn-default btn-lg glyphicon glyphicon-menu-hamburger" id="menu-toggle"></a>
			</div>
			<div class="col-xs-7" id="cameraName">Loading camera...</div>
		</div>
		<script type="text/html" id="alertHTML">
			<div class='alert alert-info flash'>
				<a href='@link' class='close' data-dismiss='alert' aria-label='close'>&times;</a>
				<strong>Info!</strong>
				@message
			</div>
		</script>
		<div id="alertContainer" class="">
			<div class="alert alert-info flash">
				<a href='@link' class='close' data-dismiss='alert' aria-label='close'>&times;</a>
				<strong>Info:</strong>
				Loading current camera settings...
			</div>
		</div>
		<!-- /#alert-message -->


		<div class="row" role="main" class="ui-content" style="text-align:center">
			<h1>Take Picture</h1>
			<button onclick="takePicture()" id="snap">
				<img src="img/aperture.png" class="img">
			</button>
		</div>
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-12">
					<h2>Current Settings</h2>
					<script type="text/html" id="settingsHTML">
						<form>
							<label for="@settingName"> @labelName </label>
							<select name="@settingName" id="@settingName" data-native-menu='false' onchange="settingChange()"> @settingLineItems </select>
						</form>
					</script>
					<script type="text/html" id="settingLineItemHTML">
						<option value="@index" @selected> @value </option>
					</script>

					<div id="settingsContainer"></div>
				</div>
			</div>
		</div>
	</div>
	<!-- /#page-content-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- Menu Toggle Script -->
    <script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
    </script>

</body>

</html>
